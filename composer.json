{
    "name": "dsxwk/webman-helper",
    "description": "Webman Framework Assistant",
    "autoload": {
        "psr-4": {
            "Dsxwk\\Framework\\WebmanHelper\\": "src/"
        },
        "files": [
            "./src/Function.php"
        ]
    },
    "authors": [
        {
            "name": "dsx",
            "email": "25076778@qq.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "dsxwk/framework": "^2.0",
        "dsxwk/annotation": "^1.0",
        "vlucas/phpdotenv": "^5.6",
        "webman/redis": "^2.1",
        "symfony/console": "^7.3",
        "webman/console": "^2.1",
        "dsxwk/tp-helper": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    },
    "extra": {
        "command-config": {
            "path": "config/plugin/webman/console/command.php"
        }
    },
    "scripts": {
        "post-package-install": [
            "Dsxwk\\Framework\\WebmanHelper\\Installer::install"
        ],
        "post-package-update": [
            "Dsxwk\\Framework\\WebmanHelper\\Installer::install"
        ]
    }
}
